<ul>
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article)%>
        <%= link_to article_path(article), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} do %>
          <i class="fa-solid fa-trash"></i>
        <% end %>
    </li>
  <% end %>
</ul>





